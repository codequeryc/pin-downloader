{
  "redirects": [
    {
      "source": "/api/fetch",
      "has": [
        {
          "type": "query",
          "key": "url"
        }
      ],
      "destination": "/api/fetch",
      "statusCode": 200
    },
    {
      "source": "/api/download",
      "has": [
        {
          "type": "query",
          "key": "id"
        }
      ],
      "destination": "/api/download",
      "statusCode": 200
    },
    {
      "source": "/(.*)",
      "destination": "https://your-site.blogspot.com",
      "statusCode": 302
    }
  ]
}
